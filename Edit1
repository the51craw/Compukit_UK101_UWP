
// Processor status register AND masks.
CARRY = 0x01;
ZERO = 0x02;
IRQ_DISABLE = 0x04;
DECIMAL_MODE = 0x08;
BRK_COMMAND = 0x10;
OVERFLOW = 0x40;
NEGATIVE = 0x80;

// Addressing modes defined:
IMPLIED = 0x0001;
ACCUMULATOR = 0x0002;
ABSOLUTE = 0x0004;
ZERO_PAGE = 0x0008;
IMMEDIATE = 0x0010;
ABS_X = 0x0020;
ABS_Y = 0x0040;
IND_X = 0x0080;
IND_Y = 0x0100;
Z_PAGE_X = 0x0200;
RELATIVE = 0x0400;
INDIRECT = 0x0800;
Z_PAGE_Y = 0x1000;
